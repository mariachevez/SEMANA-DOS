class For:
    def usoFor(self):

        listasNotas=[(20,30,40,10),(20,30,40),(30,20,10),(20,30)]
        acum,cont=0,0
        for notas in listasNotas:
            print(notas)
            acump=0
            contp=0
            for nota in notas:
                acump +=nota
                acum=acum+nota
                cont=cont+1
            print(acump/len(notas))
        print(acum/cont)
bucle = For()
bucle.usoFor()
